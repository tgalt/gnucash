msgid "<b>Account</b>"
msgstr "<b>Konto</b>"

#: gnucash/gtkbuilder/assistant-csv-trans-import.glade:942
msgid "Select a row to change the mappings"
msgstr "Odaberi redak za promjenu mapiranja"

#: gnucash/gtkbuilder/assistant-csv-trans-import.glade:960
msgid ""
